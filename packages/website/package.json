{
  "name": "@createx/website",
  "version": "1.0.0",
  "description": "CreateX Protocol Marketing Website - Landing pages and protocol documentation",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "setup:strapi": "./setup-strapi.sh",
    "strapi:dev": "cd strapi && npm run develop",
    "strapi:build": "cd strapi && npm run build",
    "strapi:start": "cd strapi && npm run start",
    "strapi:setup": "cd strapi && npm install",
    "dev:all": "concurrently \"npm run dev\" \"npm run strapi:dev\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "next lint",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "export": "next export",
    "clean": "rm -rf .next out strapi/.tmp"
  },
  "dependencies": {
    "next": "^13.4.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@next/font": "^13.4.12",
    "framer-motion": "^10.13.0",
    "lucide-react": "^0.263.1",
    "clsx": "^2.0.0",
    "tailwind-merge": "^1.14.0",
    "next-themes": "^0.2.1",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-navigation-menu": "^1.1.3",
    "react-intersection-observer": "^9.5.2",
    "axios": "^1.4.0",
    "swr": "^2.2.0",
    "wagmi": "^1.3.10",
    "viem": "^1.6.0",
    "@rainbow-me/rainbowkit": "^1.0.12",
    "ethers": "^6.7.1",
    "@tanstack/react-query": "^4.32.0",
    "markdown-to-jsx": "^7.3.2",
    "date-fns": "^2.30.0",
    "rss": "^1.2.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-next": "^13.4.12",
    "tailwindcss": "^3.3.0",
    "@tailwindcss/typography": "^0.5.9",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.27",
    "typescript": "^5.1.6",
    "jest": "^29.6.2",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^5.17.0",
    "concurrently": "^8.2.0"
  },
  "keywords": [
    "nextjs",
    "marketing",
    "website",
    "blockchain",
    "education"
  ]
}
